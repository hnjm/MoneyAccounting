<Window x:Class="Money_Accounting.money.AddMoney"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Money_Accounting.money"
        mc:Ignorable="d"
        Title="AddMoney" Height="450" Width="800">
    <Grid>
        <Button Content="Добавить монеты" HorizontalAlignment="Center" Margin="0,80,0,0" VerticalAlignment="Top" Width="250" Height="30" Click="AddCoins_Click"/>
        <Button Content="Добавить купюры" HorizontalAlignment="Center" Margin="0,250,0,0" VerticalAlignment="Top" Width="250" Height="30" Click="AddBills_Click"/>
    </Grid>
</Window>
