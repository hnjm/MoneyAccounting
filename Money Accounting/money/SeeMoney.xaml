<Window x:Class="Money_Accounting.money.SeeMoney"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Money_Accounting.money"
        mc:Ignorable="d"
        Title="SeeMoney" Height="450" Width="800">
    <Grid>
        <TextBlock HorizontalAlignment="Center" Margin="0,271,0,0" TextWrapping="Wrap" VerticalAlignment="Top"/>
        <Label Name="Coins" HorizontalAlignment="Center" Margin="0,110,0,0" VerticalAlignment="Top" Width="250" Height="30"/>
        <Label Name="Bills" HorizontalAlignment="Center" Margin="0,170,0,0" VerticalAlignment="Top" Width="250" Height="30"/>
        <Label Name="Total" HorizontalAlignment="Center" Margin="0,230,0,0" VerticalAlignment="Top" Width="250" Height="30"/>
    </Grid>
</Window>
